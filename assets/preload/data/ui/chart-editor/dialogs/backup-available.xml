<?xml version="1.0" encoding="utf-8"?>
<dialog id="backupAvailableDialog" width="475" height="150" title="Yo, wait!">
	<vbox width="100%" height="100%">
		<label text="I just found a backup of a chart available, would you like to open it?\n" width="100%" textAlign="center" />
		<spacer height="6" />
		<label id="backupTimeLabel" text="Jan 1, 1970 0:00" width="100%" textAlign="center" />
		<spacer height="100%" />
		<hbox width="100%">
			<button text="No thanks." id="dialogCancel" />
			<spacer width="100%" />
			<button text="Take me there!" id="buttonGoToFolder" />
			<spacer width="100%" />
			<button text="Open it." id="buttonOpenBackup" />
		</hbox>
	</vbox>
</dialog>
